general {
    colors = false
    interval = 1
}

order += "volume master"
order += "cpu_usage"
order += "disk /"
order += "tztime local"

cpu_usage {
    format = "CPU: %usage"
}

disk "/" {
    format = "Disk: %free"
}

tztime local {
    format = "%Y-%m-%d %H:%M:%S"
}

volume master {
    format = "â™«:%volume"
    device = default
    mixer = Master
}








